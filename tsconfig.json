{
    "compilerOptions": {
      "target": "ES2022",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
      "@pages/*": ["page-objects/*"],     
      "@components/*": ["page-objects/Components/*"],
      "@fixtures/*": ["fixtures/*"]
    }
    },
    "include": ["**/*.ts"]
  }
  